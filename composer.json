{
    "name": "wisetaillab/laravel-aws-webhooks",
    "description": "Easy webhook handler for Laravel to catch AWS SNS notifications for various services. (WiseTailLab fork with Laravel 10/11/12 support)",
    "keywords": [
        "laravel",
        "php",
        "sns",
        "aws",
        "webhooks",
        "ses",
        "webhook"
    ],
    "license": "Apache-2.0",
    "homepage": "https://github.com/wisetaillab/laravel-aws-webhooks",
    "authors": [
        {
            "name": "Alex Renoki",
            "homepage": "https://github.com/rennokki",
            "role": "Original Author"
        },
        {
            "name": "WiseTailLab",
            "homepage": "https://github.com/wisetaillab",
            "role": "Fork Maintainer"
        }
    ],
    "require": {
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "wisetaillab/laravel-sns-events": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "RenokiCo\\AwsWebhooks\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RenokiCo\\AwsWebhooks\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.5"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "RenokiCo\\AwsWebhooks\\AwsWebhooksServiceProvider"
            ]
        }
    }
}
